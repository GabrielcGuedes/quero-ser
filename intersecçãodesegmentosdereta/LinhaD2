import java.util.Objects;

public class LinhaD2 {
    private double x;
    private double y;

    public double getX() {
        return x;
    }

    public void setX(double x) {
        this.x = x;
    }

    public double getY() {
        return y;
    }

    public void setY(double y) {
        this.y = y;
    }
    
    public LinhaD2() {
    }
    
    @Override
    public boolean equals(Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (getClass() != obj.getClass()) {
            return false;
        }
        final LinhaD2 other = (LinhaD2) obj;
        if (!Objects.equals(this.x, other.x) && !Objects.equals(this.y, other.y)) {
            return false;
        }
        
        return true;
    }
    
    
}
